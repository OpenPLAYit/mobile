{
	"expo": {
		"name": "OpenPLAYit",
		"slug": "openplayit",
		"version": "1.0.0",
		"orientation": "portrait",
		"icon": "src/assets/images/icon.png",
		"scheme": "openplayit",
		"userInterfaceStyle": "dark",
		"newArchEnabled": true,
		"android": {},
		"web": {
			"bundler": "metro",
			"output": "static",
			"favicon": "src/assets/images/logo.png"
		},
		"plugins": [
			"expo-router",
			[
				"expo-splash-screen",
				{
					"image": "src/assets/images/splash-icon.png",
					"imageWidth": 200,
					"resizeMode": "contain",
					"backgroundColor": "#080101" // almost black red
				}
			],
			"expo-font",
			"expo-web-browser"
		],
		"experiments": {
			"typedRoutes": true
		}
	}
}
