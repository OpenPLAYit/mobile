{
	"expo": {
		"name": "OpenPLAYit",
		"slug": "openplayit",
		"version": "1.0.0",
		"orientation": "portrait",
		"icon": "src/assets/images/brand/icon.png",
		"scheme": "openplayit",
		"userInterfaceStyle": "dark",
		"newArchEnabled": true,
		"android": {
			"package": "com.openplayit.app",
			"edgeToEdgeEnabled": true,
			"permissions": [
				"android.permission.READ_EXTERNAL_STORAGE",
				"android.permission.WRITE_EXTERNAL_STORAGE",
				"android.permission.ACCESS_MEDIA_LOCATION"
			]
		},
		"web": {
			"bundler": "metro",
			"output": "static",
			"favicon": "src/assets/images/brand/logo.png"
		},
		"plugins": [
			"expo-router",
			[
				"expo-splash-screen",
				{
					"image": "src/assets/images/brand/splash-icon.png",
					"imageWidth": 200,
					"resizeMode": "contain",
					"backgroundColor": "#080101"
				}
			],
			"expo-font",
			"expo-web-browser",
			[
				"expo-video",
				{
					"supportsBackgroundPlayback": true,
					"supportsPictureInPicture": true
				}
			],
			[
				"expo-media-library",
				{
					"isAccessMediaLocationEnabled": true
				}
			]
		],
		"experiments": {
			"typedRoutes": true
		},
		"extra": {
			"router": {},
			"eas": {
				"projectId": "6761147d-e90e-4066-8f4e-f6c0d4d0e60d"
			}
		},
		"owner": "onyedikachi23"
	}
}
